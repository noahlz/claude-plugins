{
  "logDir": "dist",
  "tools": {
    "npm": {
      "configFile": "package.json",
      "build": {
        "command": "vite build",
        "logFile": "{logDir}/vite.log",
        "errorPattern": "(error|Error|✘|failed)"
      },
      "test": {
        "all": {
          "command": "jest",
          "logFile": "{logDir}/jest.log",
          "errorPattern": "(FAIL|●|Error:|Expected|Received)"
        },
        "single": {
          "command": "jest {testFile}",
          "logFile": "{logDir}/jest-single.log",
          "errorPattern": "(FAIL|●|Error:|Expected|Received)"
        }
      }
    },
    "maven": {
      "configFile": "pom.xml",
      "build": {
        "command": "mvn clean install --quiet --log-file {logFile}",
        "logFile": "{logDir}/maven.log",
        "errorPattern": "\\[ERROR\\]"
      },
      "test": {
        "all": {
          "command": "mvn test --quiet --log-file {logFile}",
          "logFile": "{logDir}/maven-test.log",
          "errorPattern": "\\[ERROR\\]|Tests run:.*Failures: [1-9]"
        },
        "single": {
          "command": "mvn test --quiet -Dtest={testFile} --log-file {logFile}",
          "logFile": "{logDir}/maven-test-single.log",
          "errorPattern": "\\[ERROR\\]"
        }
      }
    },
    "gradle": {
      "configFile": "build.gradle",
      "build": {
        "command": "./gradlew build --quiet",
        "logFile": "{logDir}/gradle.log",
        "errorPattern": "FAILURE|error:"
      },
      "test": {
        "all": {
          "command": "./gradlew test --quiet",
          "logFile": "{logDir}/gradle-test.log",
          "errorPattern": "FAILED|FAILURE"
        },
        "single": {
          "command": "./gradlew test --tests {testFile} --quiet",
          "logFile": "{logDir}/gradle-test-single.log",
          "errorPattern": "FAILED|FAILURE"
        }
      }
    },
    "sbt": {
      "configFile": "build.sbt",
      "build": {
        "command": "sbt clean compile",
        "logFile": "{logDir}/sbt.log",
        "errorPattern": "\\[error\\]|error:"
      },
      "test": {
        "all": {
          "command": "sbt test",
          "logFile": "{logDir}/sbt-test.log",
          "errorPattern": "\\[error\\]|FAILED|failed|error:"
        },
        "single": {
          "command": "sbt 'testOnly {testFile}'",
          "logFile": "{logDir}/sbt-test-single.log",
          "errorPattern": "\\[error\\]|FAILED|failed|error:"
        }
      }
    },
    "go": {
      "configFile": "go.mod",
      "build": {
        "command": "go build ./...",
        "logFile": "{logDir}/go.log",
        "errorPattern": "(error|cannot find|undefined)"
      },
      "test": {
        "all": {
          "command": "go test ./...",
          "logFile": "{logDir}/go-test.log",
          "errorPattern": "(FAIL|RUN|--- FAIL|panic)"
        },
        "single": {
          "command": "go test -run {testFile} ./...",
          "logFile": "{logDir}/go-test-single.log",
          "errorPattern": "(FAIL|RUN|--- FAIL|panic)"
        }
      }
    },
    "make": {
      "configFile": "Makefile",
      "build": {
        "command": "make build",
        "logFile": "{logDir}/make.log",
        "errorPattern": "Error|error:"
      }
    },
    "cargo": {
      "configFile": "Cargo.toml",
      "build": {
        "command": "cargo build --release",
        "logFile": "{logDir}/cargo.log",
        "errorPattern": "error\\["
      },
      "test": {
        "all": {
          "command": "cargo test",
          "logFile": "{logDir}/cargo-test.log",
          "errorPattern": "test result:|failures:|FAILED"
        },
        "single": {
          "command": "cargo test {testFile}",
          "logFile": "{logDir}/cargo-test-single.log",
          "errorPattern": "test result:|failures:|FAILED"
        }
      }
    },
    "rust": {
      "configFile": "Cargo.toml",
      "build": {
        "command": "cargo build",
        "logFile": "{logDir}/rust.log",
        "errorPattern": "error\\[|error:"
      },
      "test": {
        "all": {
          "command": "cargo test",
          "logFile": "{logDir}/rust-test.log",
          "errorPattern": "test result:|failures:|FAILED"
        },
        "single": {
          "command": "cargo test {testFile}",
          "logFile": "{logDir}/rust-test-single.log",
          "errorPattern": "test result:|failures:|FAILED"
        }
      }
    },
    "dotnet": {
      "configFile": "*.csproj",
      "build": {
        "command": "dotnet build",
        "logFile": "{logDir}/dotnet.log",
        "errorPattern": "error CS|FAILED"
      },
      "test": {
        "all": {
          "command": "dotnet test",
          "logFile": "{logDir}/dotnet-test.log",
          "errorPattern": "FAILED|Failed:|error CS"
        },
        "single": {
          "command": "dotnet test --filter {testFile}",
          "logFile": "{logDir}/dotnet-test-single.log",
          "errorPattern": "FAILED|Failed:|error CS"
        }
      }
    },
    "leiningen": {
      "configFile": "project.clj",
      "build": {
        "command": "lein compile",
        "logFile": "{logDir}/lein.log",
        "errorPattern": "error|ERROR|Failed|Exception"
      },
      "test": {
        "all": {
          "command": "lein test",
          "logFile": "{logDir}/lein-test.log",
          "errorPattern": "FAIL|ERROR|Exception"
        },
        "single": {
          "command": "lein test {testFile}",
          "logFile": "{logDir}/lein-test-single.log",
          "errorPattern": "FAIL|ERROR|Exception"
        }
      }
    },
    "clojure": {
      "configFile": "deps.edn",
      "build": {
        "command": "clojure -M:build",
        "logFile": "{logDir}/clojure.log",
        "errorPattern": "error|ERROR|Exception"
      },
      "test": {
        "all": {
          "command": "clojure -M:test",
          "logFile": "{logDir}/clojure-test.log",
          "errorPattern": "FAIL|ERROR|Exception"
        },
        "single": {
          "command": "clojure -M:test {testFile}",
          "logFile": "{logDir}/clojure-test-single.log",
          "errorPattern": "FAIL|ERROR|Exception"
        }
      }
    },
    "shunit2": {
      "configFile": "shunit2",
      "test": {
        "all": {
          "command": "./tests/run-all-tests.sh",
          "logFile": "{logDir}/shunit2-all.log",
          "errorPattern": "FAILED|failures=|Error"
        },
        "single": {
          "command": "bash tests/{testFile}",
          "logFile": "{logDir}/shunit2-single.log",
          "errorPattern": "FAILED|failures=|Error"
        }
      }
    }
  }
}
