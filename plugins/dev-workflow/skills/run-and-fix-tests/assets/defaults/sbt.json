{
  "outDir": "target",
  "logFile": "{outDir}/test.log",
  "build": {
    "command": "sbt clean compile",
    "logFile": "{outDir}/sbt.log",
    "errorPattern": "\\[error\\]|error:",
    "nativeOutputSupport": false
  },
  "test": {
    "all": {
      "command": "sbt test",
      "resultsPath": "{outDir}/sbt-test.txt",
      "errorPattern": "\\[error\\]|FAILED|failed|error:",
      "nativeOutputSupport": false
    },
    "single": {
      "command": "sbt 'testOnly {testFile}'",
      "resultsPath": "{outDir}/sbt-test-single.txt",
      "errorPattern": "\\[error\\]|FAILED|failed|error:",
      "nativeOutputSupport": false
    }
  }
}
