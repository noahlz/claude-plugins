{
  "outDir": "dist",
  "logFile": "{outDir}/test.log",
  "skipBuild": true,
  "test": {
    "all": {
      "command": "./tests/run-all-tests.sh",
      "resultsPath": "{outDir}/shunit2-all.txt",
      "errorPattern": "FAILED|failures=|Error",
      "nativeOutputSupport": false
    },
    "single": {
      "command": "bash tests/{testFile}",
      "resultsPath": "{outDir}/shunit2-single.txt",
      "errorPattern": "FAILED|failures=|Error",
      "nativeOutputSupport": false
    }
  }
}
