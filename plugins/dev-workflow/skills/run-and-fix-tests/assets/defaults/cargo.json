{
  "outDir": "target",
  "logFile": "{outDir}/test.log",
  "build": [
    {
      "command": "cargo build --release",
      "logFile": "{outDir}/cargo.log",
      "errorPattern": "error\\["
    }
  ],
  "test": {
    "all": {
      "command": "cargo test",
      "resultsPath": "{outDir}/cargo-test.txt",
      "errorPattern": "test result:|failures:|FAILED"
    },
    "single": {
      "command": "cargo test {testFile}",
      "resultsPath": "{outDir}/cargo-test-single.txt",
      "errorPattern": "test result:|failures:|FAILED"
    }
  }
}
