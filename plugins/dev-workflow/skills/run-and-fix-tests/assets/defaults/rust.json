{
  "outDir": "target",
  "logFile": "{outDir}/test.log",
  "build": [
    {
      "command": "cargo build",
      "logFile": "{outDir}/rust.log",
      "errorPattern": "error\\[|error:",
      "nativeOutputSupport": false
    }
  ],
  "test": {
    "all": {
      "command": "cargo test",
      "resultsPath": "{outDir}/rust-test.txt",
      "errorPattern": "test (?<testName>\\S+) \\.\\.\\.\\s+FAILED",
      "nativeOutputSupport": false
    },
    "single": {
      "command": "cargo test {testFile}",
      "resultsPath": "{outDir}/rust-test-single.txt",
      "errorPattern": "test (?<testName>\\S+) \\.\\.\\.\\s+FAILED",
      "nativeOutputSupport": false
    }
  }
}
