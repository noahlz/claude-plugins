{
  "outDir": "build",
  "logFile": "{outDir}/test.log",
  "build": [
    {
      "command": "go build ./...",
      "logFile": "{outDir}/go.log",
      "errorPattern": "(error|cannot find|undefined)",
      "nativeOutputSupport": false
    }
  ],
  "test": {
    "all": {
      "command": "go test ./...",
      "resultsPath": "{outDir}/test-results.txt",
      "errorPattern": "(FAIL|RUN|--- FAIL|panic)",
      "nativeOutputSupport": false
    },
    "single": {
      "command": "go test -run {testFile} ./...",
      "resultsPath": "{outDir}/test-single-results.txt",
      "errorPattern": "(FAIL|RUN|--- FAIL|panic)",
      "nativeOutputSupport": false
    }
  }
}
