{
  "outDir": "target",
  "logFile": "{outDir}/test.log",
  "build": [
    {
      "command": "clojure -M:build",
      "logFile": "{outDir}/clojure.log",
      "errorPattern": "error|ERROR|Exception"
    }
  ],
  "test": {
    "all": {
      "command": "clojure -M:test",
      "resultsPath": "{outDir}/clojure-test.txt",
      "errorPattern": "FAIL|ERROR|Exception"
    },
    "single": {
      "command": "clojure -M:test {testFile}",
      "resultsPath": "{outDir}/clojure-test-single.txt",
      "errorPattern": "FAIL|ERROR|Exception"
    }
  }
}
