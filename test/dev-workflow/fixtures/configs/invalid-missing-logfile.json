{
  "outDir": "dist",
  "build": [
    {
      "command": "npm run build",
      "errorPattern": "(error)"
    }
  ],
  "test": {
    "all": {
      "command": "npm test",
      "errorPattern": "(FAIL)"
    },
    "single": {
      "command": "npm test -- {file}",
      "errorPattern": "(FAIL)"
    }
  }
}
